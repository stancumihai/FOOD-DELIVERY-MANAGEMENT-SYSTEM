            prefHeight="36.0" prefWidth="81.0"
            style="-fx-background-color: red; -fx-text-fill: white; -fx-background-radius: 10px;" text="Log Out"/>
    <Label fx:id="errorLabel" layoutX="59.0" layoutY="497.0" prefHeight="30.0" prefWidth="362.0"
           style="-fx-text-fill: red;" text="Label">
        <font>
            <Font size="16.0"/>
        </font>
    </Label>
    <Spinner fx:id="idSpinner" layoutX="267.0" layoutY="114.0" prefHeight="26.0" prefWidth="82.0"/>
    <Label layoutX="268.0" layoutY="87.0" prefHeight="20.0" prefWidth="39.0" text="Id">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <CheckBox fx:id="compositeCheckBoxStart" layoutX="454.0" layoutY="152.0" mnemonicParsing="false"/>
    <Label layoutX="448.0" layoutY="132.0" text="More"/>
    <CheckBox fx:id="compositeCheckBoxEnd" layoutX="658.0" layoutY="152.0" mnemonicParsing="false"/>
    <Label layoutX="652.0" layoutY="129.0" text="Done"/>
</AnchorPane>
