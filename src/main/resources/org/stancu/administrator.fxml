    <TextField fx:id="caloriesField" layoutX="498.0" layoutY="251.0"/>
    <TextField fx:id="proteinField" layoutX="498.0" layoutY="304.0"/>
    <TextField fx:id="sodiumField" layoutX="498.0" layoutY="354.0"/>
    <Label layoutX="499.0" layoutY="128.0" prefHeight="18.0" prefWidth="29.0" text="Title">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="500.0" layoutY="176.0" prefHeight="18.0" prefWidth="46.0" text="Rating">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="498.0" layoutY="231.0" prefHeight="20.0" prefWidth="78.0" text="Calories">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="500.0" layoutY="284.0" prefHeight="18.0" prefWidth="46.0" text="Protein">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="500.0" layoutY="334.0" prefHeight="20.0" prefWidth="60.0" text="Sodium">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <TextField fx:id="fatField" layoutX="498.0" layoutY="399.0"/>
    <Label layoutX="500.0" layoutY="380.0" prefHeight="18.0" prefWidth="46.0" text="Fat">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <TextField fx:id="priceField" layoutX="498.0" layoutY="453.0"/>
    <Label layoutX="500.0" layoutY="430.0" prefHeight="18.0" prefWidth="46.0" text="Price">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <TextField fx:id="valueArea" layoutX="59.0" layoutY="173.0" prefHeight="26.0" prefWidth="362.0"/>
    <Button fx:id="generateReportButton" layoutX="59.0" layoutY="448.0" mnemonicParsing="false"
            onAction="#generateReport" prefHeight="36.0" prefWidth="130.0"
            style="-fx-background-radius: 10px; -fx-background-color: #00AFD1;" text="Generate Report">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <ChoiceBox fx:id="comboBox" layoutX="58.0" layoutY="115.0" prefWidth="150.0"/>
    <Label layoutX="59.0" layoutY="87.0" prefHeight="20.0" prefWidth="110.0" text="Choose Criteria">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="59.0" layoutY="151.0" prefHeight="20.0" prefWidth="110.0" text="Enter Value">
        <font>
            <Font size="14.0"/>
        </font>
