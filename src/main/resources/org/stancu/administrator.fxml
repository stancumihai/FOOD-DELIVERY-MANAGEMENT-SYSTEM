            fx:controller="org.stancu.presentation.controller.user.AdministratorController">
    <Pane layoutY="2.0" prefHeight="51.0" prefWidth="725.0" style="-fx-background-color: #228B22;">
        <Label layoutX="17.0" layoutY="8.0" text="Admin Window" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="25.0"/>
            </font>
        </Label>

    </Pane>
    <Pane layoutY="53.0" prefHeight="5.0" prefWidth="725.0" style="-fx-background-color: #ECF142;"/>
    <Button fx:id="addButton" layoutX="436.0" layoutY="82.0" mnemonicParsing="false" onAction="#addProduct"
            prefWidth="70.0" style="-fx-background-radius: 10px; -fx-background-color: #00AFD1;" text="Add">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Button fx:id="deleteButton" layoutX="533.0" layoutY="82.0" mnemonicParsing="false" onAction="#deleteProduct"
            prefWidth="70.0" style="-fx-background-radius: 10px; -fx-background-color: #f59965;" text="Delete">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Button fx:id="modifyButton" layoutX="631.0" layoutY="82.0" mnemonicParsing="false" onAction="#modifyProduct"
